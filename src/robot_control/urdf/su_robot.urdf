<?xml version="1.0"?>
<robot name="origins">
  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <!-- <box size=".5 .7 .0254"/> -->
        <box size=".7 .5 .0254"/>
        <origin rpy="0 0 1.57075" xyz="0 0 0"/>
      </geometry>
    </visual>
  </link>


  <link name="shoulder_joint">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.02"/>
      </geometry>
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="black"/>
    </visual>
  </link>


  <link name="elbow">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.02"/>
      </geometry>
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="black"/>
    </visual>
  </link>


 <link name="upper_arm">
    <visual>
      <geometry>
        <box size= "0.075 0.02 0.002"/>
      </geometry>
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="black"/>
    </visual>
  </link>


 <link name="forearm">
    <visual>
      <geometry>
        <box size= "0.075 0.02 0.002"/>
      </geometry>
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="white"/>
    </visual>
  </link>

 <link name="shoulder">
    <visual>
      <geometry>
        <box size= "0.075 0.02 0.002"/>
      </geometry>
      <!-- <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/> -->
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="blue"/>
    </visual>
  </link>


  <link name="right_front_wheel">
    <visual>
      <geometry>
        <cylinder length="0.01" radius="0.07"/>
      </geometry>
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="blue"/>
    </visual>
  </link>


  <link name="left_front_wheel">
    <visual>
      <geometry>
        <cylinder length="0.01" radius="0.07"/>
      </geometry>
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
    </visual>
  </link>


  <link name="right_back_wheel">
    <visual>
      <geometry>
        <cylinder length="0.01" radius="0.07"/>
      </geometry>
      <!--
      <origin rpy="0 1.57075 0" xyz="-0.12 0.05 0.0"/>
      -->
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="blue"/>
    </visual>
  </link>

  <link name="left_back_wheel">
    <visual>
      <geometry>
        <cylinder length="0.01" radius="0.07"/>
      </geometry>
      <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <material name ="black"/>
    </visual>
  </link>

  <!-- CAM -->
   <link name="camera">
     <visual>
       <geometry>
         <cylinder length="0.01" radius="0.07"/>
       </geometry>
       <origin rpy="0 1.57075 0" xyz="0 0 0.06"/>
       <material name ="black"/>
     </visual>
   </link>




  <!-- JOINTS -->

  <!-- WHEELS -->
  <joint name="base_to_front_right_leg" type="continuous">
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <!-- <origin rpy="0 0 1.57075" xyz="0.16 0.26 0.0"/> -->
    <!-- <origin rpy="0 0 0" xyz="-0.16 -0.26 0.0"/> -->
    <origin rpy="0 0 3.14159" xyz="-0.16 -0.26 0.0"/>
    <!-- <axis xyz="1 0 0"/> -->
    <axis xyz="0 1 0"/>
    <material name ="black"/>
  </joint>

  <joint name="base_to_front_left_leg" type="continuous">
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <!-- <origin rpy="0 0 1.57075" xyz="0.16 0.26 0.0"/> -->
    <origin rpy="0 0 0" xyz="-0.16 0.26 0.0"/>
    <!-- <axis xyz="1 0 0"/> -->
    <axis xyz="0 1 0"/>
    <material name ="white"/>
  </joint>


  <joint name="base_to_back_right_leg" type="fixed">
    <parent link="base_link"/>
    <child link="right_back_wheel"/>
    <origin rpy="0 0 0" xyz="0.16 -0.26 0.0"/>
    <axis xyz="0 1 0"/>
    <material name ="black"/>
  </joint>


  <joint name="base_to_back_left_leg" type="fixed">
    <parent link="base_link"/>
    <child link="left_back_wheel"/>
    <origin rpy="0 0 0" xyz="0.16 0.26 0.0"/>
    <axis xyz="0 1 0"/>
    <material name ="white"/>
  </joint>


  <!-- ARM -->
  <joint name="base_to_shoulder" type="fixed">
    <parent link="base_link"/>
    <child link="shoulder"/>
    <!-- xyz=000 -->
    <origin xyz="-0.17 0 0.045"/>
    <material name ="white"/>
  </joint>

  <joint name="shoulder_to_shoulder_joint" type="continuous">
    <parent link="shoulder"/>
    <child link="shoulder_joint"/>
    <!--
    <origin xyz="0 0.0 0.0"/>
    -->
    <origin rpy="0 0 0" xyz="0 0 0.05"/>
    <axis xyz="0 1 0"/>
    <material name ="white"/>
  </joint>

  <joint name="shoulder_joint_to_upper_arm" type="fixed">
    <parent link="shoulder_joint"/>
    <child link="upper_arm"/>
    <origin xyz="0 0.0 0.05"/>
    <material name ="white"/>
  </joint>

  <joint name="upper_arm_to_elbow" type="continuous">
    <parent link="upper_arm"/>
    <child link="elbow"/>
    <origin rpy="0 0 0" xyz="0 0.0 0.04"/>
    <axis xyz="0 1 0"/>
    <material name ="white"/>
  </joint>

  <joint name="elbow_to_forearm" type="fixed">
    <parent link="elbow"/>
    <child link="forearm"/>

    <!--
    <origin rpy="0 1.57075 0" xyz="0 -0.16 0.05"/>
    <origin rpy="0 0 0" xyz="0.05 0.16 -0.058"/>
    -->
   
     <origin rpy="0 0 0" xyz="0 0 0.05"/>
    <axis xyz="0 0 0"/>
    <material name="white"/>
  </joint>

<!--   ROS2 Control tags   -->
<!--  <ros2_control name="arduino_control" type="system">    -->
  <ros2_control name="robot_control" type="system">

      <hardware>
          <plugin>topic_based_ros2_control/TopicBasedSystem</plugin>
          <param name="joint_commands_topic">/arduino/joint_state_setpoint</param>
          <param name="joint_states_topic">/arduino/joint_state_motor</param>
          <param name="trigger_joint_command_threshold">-1.0</param>
      </hardware>

      <joint name="base_to_front_left_leg">
          <command_interface name="velocity"/>
          <state_interface name="position">
              <param name="initial_value">0.0</param>
          </state_interface>
          <state_interface name="velocity"/>
      </joint>

      <joint name="base_to_front_right_leg">
          <command_interface name="velocity"/>
          <state_interface name="position">
              <param name="initial_value">0.0</param>
          </state_interface>
          <state_interface name="velocity"/>
      </joint>

      <joint name="shoulder_to_shoulder_joint">
          <command_interface name="velocity"/>
          <state_interface name="position">
              <param name="initial_value">0.0</param>
          </state_interface>
          <state_interface name="velocity"/>
      </joint>

      <joint name="upper_arm_to_elbow">
          <command_interface name="velocity"/>
          <state_interface name="position">
              <param name="initial_value">0.0</param>
          </state_interface>
          <state_interface name="velocity"/>
      </joint>

  </ros2_control>


</robot>
